#S1

conf t
hostname S1
vlan 2
name VLAN2
vlan 3
name VLAN3
exit
int f0/1
switchport mode trunk
no shutdown
exit
int f0/2
switchport mode trunk
no shutdown
exit
int vlan 2
ip add 10.0.2.2 255.255.255.0
no shutdown
exit
ip default-gateway 10.0.0.1
end
wr

#S2

conf t
hostname S2
vlan 2
name VLAN2
vlan 3
name VLAN3
exit
int f0/2
switchport mode trunk
no shutdown
exit
int vlan 3
ip add 10.0.3.2 255.255.255.0
no shutdown
exit
ip default-gateway 10.0.0.1
end
wr
